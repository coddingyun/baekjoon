#include <iostream>
#include <string.h>

int main(){
    using namespace std;
    string S;
    cin >> S;
    for (size_t i=0; i<S.size();i++){
        switch (S[i]){
        case 'A':
        case 'B':
        case 'C':
            S[i]=3;
            break;
        case 'D':
        case 'E':
        case 'F':
            S[i]=4;
            break;
        case 'G':
        case 'H':
        case 'I':
            S[i]=5;
            break;
        case 'J':
        case 'K':
        case 'L':
            S[i]=6;
            break;
        case 'M':
        case 'N':
        case 'O':
            S[i]=7;
            break;
        case 'P':
        case 'Q':
        case 'R':
        case 'S':
            S[i]=8;
            break;
        case 'T':
        case 'U':
        case 'V':
            S[i]=9;
            break;
        case 'W':
        case 'X':
        case 'Y':
        case 'Z':
            S[i]=10;
            break;
        }
    }
    int sum=0;
    for (size_t i=0; i<S.size();i++){
        sum+=(int)S[i];
    }
    cout << sum << endl;
}
